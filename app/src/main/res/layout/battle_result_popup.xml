<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/pop_li_width"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".battle_result_popup">

    <TextView
        android:id="@+id/popup_title"
        android:layout_marginTop="@dimen/pop_margin"
        android:layout_marginLeft="@dimen/pop_margin"
        android:text="결과"
        android:textSize="@dimen/pop_title"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <ImageView
        android:id="@+id/popup_content"
        android:layout_gravity="center_horizontal"
        android:layout_width="250dp"
        android:layout_height="250dp" />



</LinearLayout>